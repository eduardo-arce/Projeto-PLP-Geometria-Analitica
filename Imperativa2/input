{ 
   var calcDist = dist([10,0,1,5]),
   var calcPm = pm([-10,0,11,-5]),
   var calcAng = ang([1,1,-2,-2,1,1]),
   var calcGraus2Rad = graus2rad(60),
   var calcRad2Graus = rad2graus(1.0472);

   write(calcDist);
   write(calcPm);
   write(calcAng);
   write(calcGraus2Rad);
   write(calcRad2Graus)
}